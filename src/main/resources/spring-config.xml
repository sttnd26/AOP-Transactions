<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/aop
        http://www.springframework.org/schema/aop/spring-aop.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <context:component-scan base-package="springQ1"/>
    <context:component-scan base-package="springQ2"/>
    <context:component-scan base-package="springQ3"/>
    <context:component-scan base-package="springQ4"/>
    <context:component-scan base-package="springQ5"/>
    <context:component-scan base-package="springQ6"/>
    <context:component-scan base-package="springQ7"/>

    <aop:aspectj-autoproxy/>

    <bean class="springQ1.MyAspect"/>
    <bean class="springQ2.MyAspect"/>
    <bean class="springQ3.MyAspect"/>
    <bean class="springQ4.MyAspect"/>
    <bean class="springQ5.MyAspect"/>
    <bean class="springQ6.MyAspect"/>
    <bean class="springQ7.MyAspect"/>

</beans>